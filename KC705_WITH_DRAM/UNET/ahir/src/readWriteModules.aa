$pipeline $depth 8 $buffering 1 $fullrate
$opaque $module [writeModule_zeropad]
$in (index : $uint<8> address : $uint<32> data : $uint<64>)
$out (done : $uint<1>)
$is
{
	cnd1 := (index == 0)
	cnd2 := (index == 1)
	$guard (cnd1) $call memoryModule (0 (T0_base + address) data) (out1)
	$guard (cnd2) $call memoryModule (0 (T1_base + address) data) (out2)
	done := (index [] 0)
}

$pipeline $depth 8 $buffering 1 $fullrate
$opaque $module [readModule_zeropad]
$in (index : $uint<8> address : $uint<32>)
$out (data : $uint<64>)
$is
{
	cnd1 := (index == 0)
	cnd2 := (index == 1)
	$guard (cnd1) $call memoryModule (1 (T0_base + address) 0) (val1)
	$guard (cnd2) $call memoryModule (1 (T1_base + address) 0) (val2)
	
	data := ( $excmux 
				cnd1 val1 cnd2 val2
			 )
	
}

$pipeline $depth 8 $buffering 1 $fullrate
$opaque $module [writeModule_concat]
$in (index : $uint<8> address : $uint<32> data : $uint<64>)
$out (done : $uint<1>)
$is
{
	cnd1 := (index == 0)
	cnd2 := (index == 1)
