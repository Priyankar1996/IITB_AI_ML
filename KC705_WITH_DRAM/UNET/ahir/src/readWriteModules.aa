	$guard (cnd2) $call memoryModule (0 (T1_base + address) data) (out2)
	done := (index [] 0)	
}

$pipeline $depth 8 $buffering 1 $fullrate
$opaque $module [readModule_concat]
$in (index : $uint<8> address : $uint<32>)
$out (data : $uint<64>)
$is
{
	cnd1 := (index == 0)
	cnd2 := (index == 1)
	cnd3 := (index == 2)
	cnd4 := (index == 3)
	cnd5 := (index == 4)
	$guard (cnd1) $call memoryModule (1 (T0_base + address) 0) (val1)
	$guard (cnd2) $call memoryModule (1 (T1_base + address) 0) (val2)
	$guard (cnd1) $call memoryModule (1 (T2_base + address) 0) (val3)
	$guard (cnd2) $call memoryModule (1 (T3_base + address) 0) (val4)
	$guard (cnd1) $call memoryModule (1 (T4_base + address) 0) (val5)
	
	data := ( $excmux cnd1 val1 cnd2 val2 cnd3 val3
					  cnd4 val4 cnd5 val5 )
}

