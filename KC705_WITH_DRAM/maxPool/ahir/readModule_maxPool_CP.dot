digraph control_path {
  e_0: entry_node : n ;
  e_1 [shape = dot];
  e_2 [shape = dot];
  e_3 [shape = invtrapezium];
  e_4 [shape = dot];
  e_5 [shape = invtriangle];
  e_6 [shape = invtriangle];
  e_7 [shape = triangle];
  e_8 [shape = triangle];
  e_9 [shape = dot];
  e_10 [shape = dot];
  e_11 [shape = dot];
  e_12: exit_node : s ;
  e_0 -> e_1;
  e_7 -> e_3[style = dashed];
  e_11 -> e_4;
  e_1 -> e_5;
  e_7 -> e_5[style = dashed];
  e_4 -> e_6;
  e_8 -> e_6[style = dashed];
  e_5 -> e_7;
  e_6 -> e_8;
  e_2 -> e_9;
  e_3 -> e_10;
  e_8 -> e_12;
}
